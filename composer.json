{
  "name": "easyphp/easyphp",
  "description": "超级轻量级 php api框架",
  "keywords": [
    "php框架",
    "swoole",
    "轻量级",
    "高性能"
  ],
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "ljk",
      "email": "303147190@qq.com"
    }
  ],
  "require": {
    "php": ">=7.0.0",
    "ext-pdo": "*",
    "ext-redis": "*",
    "ext-json": "*"
  },
  "autoload": {
    "psr-4": {
      "easy\\": "src/easy/",
      "easy\\traits\\": "src/traits/",
      "easy\\utils\\": "src/utils/"
    }
  },
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  }
}
